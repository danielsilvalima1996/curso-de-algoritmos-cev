algoritmo "semnome"
var
   Nome: caractere
   Idade: real

   
inicio

      escreva("Qual seu nome: ", Nome)
      Leia (Nome)
      Escreva ("Qual sua idade: ", Idade)
      Leia(Idade)
      Escreva("Você é ", Nome)
      escrevaL(" e tem", Idade, " idade.")
      Escreval("PARABÉNS!")
      
fimalgoritmo
