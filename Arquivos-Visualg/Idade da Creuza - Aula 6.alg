algoritmo "Creuza_01"
var

   Ano_atual,Ano_nasci, idade: inteiro
   
inicio

      Escreva ("Em que ano nós estamos? ")
      Leia (Ano_atual)
      Escreva ("Em que ano eu nasci? ")
      Leia (Ano_nasci)
      idade <- ano_atual - Ano_nasci
      Escreva ("A minha idade é ", idade)
fimalgoritmo
